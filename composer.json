{
  "name": "duxphp/duxravel-admin",
  "description": "Duxravel Basic Admin Application",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "DuxPHP",
      "email": "admin@duxphp.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "duxphp/duxravel-app": "*"

  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "Modules\\System\\": "src/system",
      "Modules\\Tools\\": "src/tools",
      "Duxravel\\System\\Seeders\\": "database/seeders"
    }
  },
  "extra": {
    "branch-alias": {
    },
    "laravel": {
      "providers": [
        "Modules\\System\\Providers\\AdminServiceProvider",
        "Modules\\Tools\\Providers\\ToolsServiceProvider"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  }
}